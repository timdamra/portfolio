{
"extends": "airbnb",
"parserOptions": {
  "ecmaVersion": 6,
  "ecmaFeatures": {
    "jsx": true
    }
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": 1,
      "comma-dangle": 0,
      "linebreak-style": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "arrow-parens": ["error", "as-needed"],
      "arrow-body-style": ["warn", "as-needed"],
      "react/prop-types": 0
      }
}
